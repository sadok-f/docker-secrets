    "keywords": [
        "docker secrets", "php", "php docker secrets", "docker secret"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sadok Ferjani",
            "email": "sadoknet@gmail.com",
            "homepage": "http://sadokferjani.me",
            "role": "Developer"
        }
